
#get-paypal.modal.fade
	.modal-dialog
		.modal-content
			.modal-header
				button.close(data-dismiss='modal') x
				h2(style='text-align : center') Withdraw
				label(style='color:#201e22;font-size: 20px;text-align : center')#label
			.modal-body
				#get-paypal-form
					.row(style='text-align: center')
						a(href='https://paytm.com/').btn.btn-primary.button-sign-in
							span.glyphicon.glyphicon-play
							| paytm
					.row(style='text-align: center;margin-top:20px')
						a(href='https://www.paypal.com').btn.btn-primary.button-sign-in
							span.glyphicon.glyphicon-play
							|paypal
					.row(style='text-align: center;margin-top:20px')
						button(method="post",href="/payment").btn.button-sign-in.btn-danger
							span.glyphicon.glyphicon-remove
							| Delete withdraw
			.modal-footer
					button(data-dismiss="modal")#cancel.btn.btn-default Cancel
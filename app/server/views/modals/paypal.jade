
#get-paypal.modal.fade
	.modal-dialog
		.modal-content
			.modal-header

				button.close(data-dismiss='modal') x
				h2(style='text-align : center') Withdraw
				label(style='color:#201e22;font-size: 20px;text-align : center')  withdraw
				label(style='color:#FF0000;font-size: 20px;text-align : center')#label_amount
				label(style='color:#201e22;font-size: 20px;text-align : center')  to
				label(style='color:#FF0000;font-size: 20px;text-align : center')#label
			.modal-body
				#get-paypal-form
					.row(style='text-align: center')
						a(href='https://paytm.com/',id='paytm').btn.btn-primary.button-sign-in
							span.glyphicon.glyphicon-play
							| paytm
					.row(style='text-align: center;margin-top:20px')
						a(href='https://www.paypal.com',id='paypal').btn.btn-primary.button-sign-in
							span.glyphicon.glyphicon-play
							|paypal
					.row(style='text-align: center;margin-top:20px')

			.modal-footer
					button(data-dismiss="modal")#cancel.btn.btn-default Cancel